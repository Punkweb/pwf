{
  "name": "pwf",
  "version": "0.3.1",
  "description": "Punkweb client-side JavaScript framework for building Single Page Applications",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "parcel example/index.html",
    "build": "tsc --watch",
    "format": "prettier --write 'src/**/*.{html,css,scss,js,ts,md}'",
    "test": "jest src/ --coverage",
    "nuke": "rm -rf .cache dist lib node_modules",
    "nuke:dist": "rm -rf .cache dist",
    "reinstall": "npm run nuke && npm install",
    "prepare": "rm -rf lib && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:Punkweb/pwf.git"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [],
  "author": "Punkweb (https://github.com/Punkweb/)",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/Punkweb/pwf/issues",
  "homepage": "https://github.com/Punkweb/pwf#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@parcel/transformer-sass": "^2.7.0",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.5",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "rxjs": "^7.5.6",
    "typescript": "^4.7.4"
  }
}
